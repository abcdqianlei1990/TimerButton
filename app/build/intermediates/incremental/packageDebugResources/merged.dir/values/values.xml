<?xml version="1.0" encoding="utf-8"?>
<resources>
    <color name="colorAccent">#FF4081</color>
    <color name="colorPrimary">#3F51B5</color>
    <color name="colorPrimaryDark">#303F9F</color>
    <string name="app_name">TimerButton</string>
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>
    <declare-styleable name="TimerButton">
        <attr format="boolean" name="TimerButton_autoCounting"/>
        <attr format="integer" name="TimerButton_from"/>
        <attr format="color" name="TimerButton_countingTextColor"/>
        <attr format="reference" name="TimerButton_countingBackground"/>
    </declare-styleable>
</resources>